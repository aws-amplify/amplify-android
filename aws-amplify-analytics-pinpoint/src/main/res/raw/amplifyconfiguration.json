{
  "UserAgent": "aws-amplify-cli/2.0",
  "Version": "1.0",
  "Storage": {
    "plugins": {
      "AWSS3StoragePlugin": {
        "Bucket": "my-s3-bucket",
        "Region": "us-west-2",
        "TransferAcceleration": true
      }
    }
  },
  "Analytics": {
    "plugins": {
      "AmazonPinpointAnalyticsPlugin": {
        "appId": "3d052968a8514a778b5a87891a98a4b7",
        "region": "us-west-2",
        "autoFlushEventsInterval" : 30,
        "autoSessionTrackingInterval": 30,
        "trackAppLifecycleEvents" : true
      }
    }
  }
}

